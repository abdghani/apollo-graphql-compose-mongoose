{
  "name": "express-graphql-mongodb-boilerplate",
  "version": "1.0.0",
  "description": "A boilerplate for Node.js apps / GraphQL-Apollo / Authentication from scratch - express, graphql - (graphql compose), mongodb (mongoose).",
  "main": "./src",
  "repository": {
    "type": "git",
    "url": "https://github.com/abdghani/apollo-graphql-compose-mongoose"
  },
  "bugs": "https://github.com/abdghani/apollo-graphql-compose-mongoose/issues",
  "keywords": [
    "express",
    "graphql",
    "graphql-compose",
    "mongodb",
    "mongoose",
    "node",
    "nodejs",
    "authentication",
    "scratch",
    "boilerplate",
    "authorization",
    "docker",
    "api"
  ],
  "scripts": {
    "start": "cross-env NODE_PATH=./src node ./src",
    "start:watch": "cross-env NODE_PATH=./src nodemon ./src",
    "lint": "eslint --debug ./src",
    "lint:fix": "eslint --debug ./src --fix",
    "prettier": "prettier \"**/*.+(js|jsx|json|css|md)\"",
    "prettier:fix": "prettier --write \"**/*.+(js|jsx|json|css|md)\""
  },
  "engines": {
    "node": "18.x"
  },
  "author": "Abdullah Ghani",
  "license": "MIT",
  "dependencies": {
    "@apollo/server": "^4.7.1",
    "@graphql-tools/schema": "^9.0.19",
    "bcryptjs": "^2.4.3",
    "check-types": "^11.1.2",
    "cors": "^2.8.5",
    "crypto-random-string": "^3.3.0",
    "dotenv": "^8.6.0",
    "ejs": "^3.1.5",
    "express": "^4.18.2",
    "graphql": "^16.6.0",
    "graphql-compose": "^9.0.10",
    "graphql-compose-mongoose": "^9.8.0",
    "graphql-redis-subscriptions": "^2.6.0",
    "graphql-ws": "^5.13.1",
    "hashids": "^2.2.8",
    "ioredis": "^4.19.2",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "module-alias": "^2.2.2",
    "moment": "^2.29.1",
    "mongoose": "^6.0.10",
    "validator": "^13.5.2",
    "winston": "^3.3.3",
    "ws": "^8.13.0"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "eslint": "^7.15.0",
    "eslint-config-prettier": "^6.14.0",
    "eslint-plugin-prettier": "^3.1.4",
    "prettier": "^2.2.1"
  }
}
